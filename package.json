{
  "name": "simpler-lexer",
  "version": "0.1.3",
  "description": "A simple lexer that parses strings into tokens using user-defined regular expressions",
  "keywords": [
    "simple",
    "lexer",
    "parser",
    "token",
    "tokens",
    "tokenizer",
    "regexp",
    "regular",
    "expressions"
  ],
  "main": "lib/index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel -d lib/ src/",
    "test": "npm run compile && ./node_modules/.bin/mocha --compilers js:babel-core/register",
    "coverage": "npm run compile && ./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha --",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bananushka/simpler-lexer.git"
  },
  "author": "bananushka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bananushka/simpler-lexer/issues"
  },
  "homepage": "https://github.com/bananushka/simpler-lexer#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-istanbul": "^0.5.9",
    "babel-preset-es2015": "^6.3.13",
    "mocha": "^2.3.4"
  },
  "dependencies": {}
}
